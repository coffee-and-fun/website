{% assign showDonation = showDonation | default: false %}
{% assign showTriviaApp = showTriviaApp | default: false %}
{% assign showMysteryApp = showMysteryApp | default: false %}
{% assign showKeyboardModal = showKeyboardModal | default: false %}
{% assign showInstantApp = showInstantApp | default: false %}
{% assign showProcrastinot = showProcrastinot | default: false %}
{% assign showCannon = showCannon | default: false %}
{% assign showMiddleBadge = showMiddleBadge | default: false %}


{% include  ticker.liquid %}

<header class="navbar brand-color-background">

    <div class="navbar-start">
        {% if showDonation %}
        <a class="btn btn-secondary btn-sm" href="https://payments.coffeeandfun.com/b/00gbKHdise1e8Le6oZ"
            title="Donate to Coffee & Fun LLC" target="_blank" aria-label="Donate to Coffee & Fun LLC"
            aria-controls="my_modal_site_menu" role="menuitem">
            Donate
        </a>
        {% endif %}
        {% if showCreditsBuyButton %}
        <a class="btn btn-secondary btn-sm" href="https://payments.coffeeandfun.com/b/14k4ifa6gbT62mQ9Bc"
            title="Donate to Coffee & Fun LLC" target="_blank" aria-label="Donate to Coffee & Fun LLC"
            aria-controls="my_modal_site_menu" role="menuitem">
            Secure a Spot
        </a>
        {% endif %}

    </div>

    <div class="navbar-center">


        {% if showMiddleBadge %}
    

        <a href="/" class="btn btn-ghost gap-1 px-2 md:gap-2">
            <img alt="Logo of Coffee & Fun LLC featuring a stylized cup with a coffee bean pattern, representing the company's focus on creativity and innovation in software development."
            src="/assets/images/favicon.png" class="h-10 w-10 aspect-square" decoding="async" data-nimg="future"
                loading="lazy" />
            <span class="sr-only font-title text-base-content text-lg md:text-2xl">Coffee & Fun</span>
        </a>
        {% endif %}





        

        {% if showProcrastinot %}
       
            <a href="/" class="btn btn-ghost gap-1 px-2 md:gap-2">
                <img alt="Instant Incognito automatically opens specific websites in incognito mode for enhanced privacy."
                    src="/assets/images/apps/procrastinot.png" class="h-10 w-10 aspect-square" decoding="async" data-nimg="future"
                    loading="lazy" />
                <span class="font-title text-base-content text-lg md:text-2xl">ProcrastiNot</span>
            </a>
 
        {% endif %}

        {% if showInstantApp %}
       
            <a href="/" class="btn btn-ghost gap-1 px-2 md:gap-2">
                <img alt="Instant Incognito automatically opens specific websites in incognito mode for enhanced privacy."
                    src="/assets/images/instant-incognito.png" class="h-10 w-10 aspect-square" decoding="async" data-nimg="future"
                    loading="lazy" />
                <span class="font-title text-base-content text-lg md:text-2xl">Instant Incognito</span>
            </a>
 
        {% endif %}


        {% if showMysteryApp %}
        <div class="tooltip tooltip-open tooltip-bottom"
            data-tip="The more you pay, the higher you'll rank, but the amounts others paid will remain a mystery.">
            <a href="/" class="btn btn-ghost gap-1 px-2 md:gap-2">
                <img alt="Mystery Rank - Compete, Contribute, and Climb the Ranks with a Twist"
                    src="/assets/images/star.png" class="h-10 w-10 aspect-square" decoding="async" data-nimg="future"
                    loading="lazy" />
                <span class="font-title text-base-content text-lg md:text-2xl">Mystery Rank</span>
            </a>
        </div>
        {% endif %}
    </div>

    <div class="navbar-end">

        {% if showKeyboardModal %}
        <button class="btn btn-ghost btn-circle" onclick="my_modal_69.showModal()" title="Open the About modal"
            aria-label="Open the About modal" aria-controls="my_modal_69" role="menuitem">

            <svg xmlns="http://www.w3.org/2000/svg" class="w-6 h-6" fill="currentColor" viewBox="0 -960 960 960">
                <path
                    d="M120-120q-33 0-56.5-23.5T40-200v-520q0-33 23.5-56.5T120-800h720q33 0 56.5 23.5T920-720v520q0 33-23.5 56.5T840-120H120Zm0-80h720v-520H120v520Zm200-80h320v-40H320v40ZM200-400h80v-80h-80v80Zm160 0h80v-80h-80v80Zm160 0h80v-80h-80v80Zm160 0h80v-80h-80v80ZM200-560h80v-80h-80v80Zm160 0h80v-80h-80v80Zm160 0h80v-80h-80v80Zm160 0h80v-80h-80v80ZM120-200v-520 520Z" />
            </svg>
        </button>
        {% endif %}


        {% if showMysteryApp %}
        <button class="btn btn-ghost btn-circle" onclick="my_modal_rank.showModal()"
            title="Learn more about Mystery Rank" aria-label="Learn more about Mystery Rank"
            aria-controls="my_modal_rank" role="menuitem">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 -960 960 960" class="w-6 h-6" fill="currentColor">
                <path
                    d="M280-280h280v-80H280v80Zm0-160h400v-80H280v80Zm0-160h400v-80H280v80Zm-80 480q-33 0-56.5-23.5T120-200v-560q0-33 23.5-56.5T200-840h560q33 0 56.5 23.5T840-760v560q0 33-23.5 56.5T760-120H200Zm0-80h560v-560H200v560Zm0-560v560-560Z" />
            </svg>
        </button>
        {% endif %}


        {% if showCannon %}
        <button 
            class="btn btn-secondary btn-sm btn-circle mr-2" 
            onclick="launchConfetti()"
            title="Party" 
            aria-label="Party"
            aria-controls="Party" 
            role="menuitem">
        
                <svg xmlns="http://www.w3.org/2000/svg" enable-background="new 0 0 24 24"  viewBox="0 0 24 24" class="w-6 h-6" fill="currentColor"><g><rect fill="none" height="24" width="24"/></g><g><g><polygon points="2,22 16,17 7,8"/><path d="M14.53,12.53l5.59-5.59c0.49-0.49,1.28-0.49,1.77,0l0.59,0.59l1.06-1.06l-0.59-0.59c-1.07-1.07-2.82-1.07-3.89,0 l-5.59,5.59L14.53,12.53z"/><path d="M10.06,6.88L9.47,7.47l1.06,1.06l0.59-0.59c1.07-1.07,1.07-2.82,0-3.89l-0.59-0.59L9.47,4.53l0.59,0.59 C10.54,5.6,10.54,6.4,10.06,6.88z"/><path d="M17.06,11.88l-1.59,1.59l1.06,1.06l1.59-1.59c0.49-0.49,1.28-0.49,1.77,0l1.61,1.61l1.06-1.06l-1.61-1.61 C19.87,10.81,18.13,10.81,17.06,11.88z"/><path d="M15.06,5.88l-3.59,3.59l1.06,1.06l3.59-3.59c1.07-1.07,1.07-2.82,0-3.89l-1.59-1.59l-1.06,1.06l1.59,1.59 C15.54,4.6,15.54,5.4,15.06,5.88z"/></g></g></svg>
        </button>

        {% endif %}


      

        <button 
            class="btn btn-ghost btn-circle ml-2" 
            onclick="my_modal_site_menu.showModal()"
            title="Open the contact support form" 
            aria-label="Open the contact support form"
            aria-controls="my_modal_site_menu" 
            role="menuitem">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 -960 960 960" class="w-8 h-8" fill="currentColor">
                <path d="M120-240v-80h720v80H120Zm0-200v-80h720v80H120Zm0-200v-80h720v80H120Z" />
            </svg>
        </button>


    </div>
</header>
